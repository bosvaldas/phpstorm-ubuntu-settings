<application>
  <component name="PostfixTemplates">
    <template id="tc@userDefined" key=".tc" provider="builtin.php" topmost="false">
      <conditions />
      <template name="fakeKey" value="try {&#10;    $EXPR$;&#10;} catch (\Throwable $e) {&#10;    dd($e);&#10;}" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="dd@userDefined" key=".dd" provider="builtin.php" topmost="false">
      <conditions />
      <template name="fakeKey" value="dd($EXPR$);" description="" toReformat="true" toShortenFQNames="true" />
    </template>
  </component>
  <component name="PostfixTemplatesSettings">
    <option name="providerToDisabledTemplates">
      <disabled-templates provider="builtin.php">
        <value>
          <set>
            <option value="com.jetbrains.php.postfixCompletion.PhpEchoPostfixTemplate#.echo" />
            <option value="com.jetbrains.php.postfixCompletion.PhpElsePostfixTemplate#.else" />
            <option value="com.jetbrains.php.postfixCompletion.PhpIntroduceVarPostfixTemplate#.var" />
            <option value="com.jetbrains.php.postfixCompletion.PhpIsNotNullPostfixTemplate#.nn" />
            <option value="com.jetbrains.php.postfixCompletion.PhpIsNotNullPostfixTemplate#.notnull" />
            <option value="com.jetbrains.php.postfixCompletion.PhpIsNullPostfixTemplate#.null" />
            <option value="com.jetbrains.php.postfixCompletion.PhpIssetPostfixTemplate#.isset" />
            <option value="com.jetbrains.php.postfixCompletion.PhpNotPostfixTemplate#!" />
            <option value="com.jetbrains.php.postfixCompletion.PhpNotPostfixTemplate#.not" />
            <option value="com.jetbrains.php.postfixCompletion.PhpParenPostfixTemplate#.par" />
            <option value="com.jetbrains.php.postfixCompletion.PhpReturnPostfixTemplate#.return" />
            <option value="com.jetbrains.php.postfixCompletion.PhpThrowPostfixTemplate#.throw" />
            <option value="com.jetbrains.php.postfixCompletion.PhpVarDumpPostfixTemplate#.var_dump" />
          </set>
        </value>
      </disabled-templates>
    </option>
  </component>
</application>